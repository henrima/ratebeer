<p id="notice"><%= notice %></p>

<h1>Listing ratings</h1>
<p> Number of ratings: <%= @ratings.count %> </p>


<table>
  <tbody>
	<ul>
 	<% @ratings.each do |rating| %>
   		<li> <%= rating %> <%= link_to rating.user.username, user_path(rating.user.id) %> </li>
 	<% end %>
	</ul>
  </tbody>
</table>

  	<%= link_to 'New Rating', '/ratings/new' %>

<br>
